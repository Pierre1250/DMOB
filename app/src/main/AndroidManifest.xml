<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projetmobile">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="androidx.multidex.MultiDexApplication"
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Mon_Panier" android:label="Commande actuelle : ">

        </activity>
        <activity android:name=".CreerCompte"
            android:label="Creer un compte">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Ajouterproduit"
            android:label="Ajouter un produit">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".CreerCompte" />
        </activity>
        <activity android:name=".Se_Connecter" />
        <activity android:name=".MainActivity"
            android:label="Nos produit">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".CreerCompte" />
        </activity>
    </application>

</manifest>